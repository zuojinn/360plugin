{
    "name": "本地测试收藏浏览器当前URL插件",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "本地测试收藏浏览器当前URL插件，有问题请联系IT部",
    "icons": {
        "128": "icon.png"
    },
	"permissions": [
		"tabs","cookies","storage"
	],

	"optional_permissions": ["tabs","http://*/*", "https://*/*","<all_urls>"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
}